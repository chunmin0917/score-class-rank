{
    "name": "抓取某人成绩的班级名次",
    "version": "0.3",
    "manifest_version": 3,
    "description": "自动查出班级所有学生的成绩，然后统计出某人的班级名次。用户必须提供班级所有学生的姓名。",
    "default_locale": "zh_CN",
    "icons": {
        "128": "icon128.png"
    },
   "side_panel": {
        "default_path": "side_panel/sidepanel.html"
    },
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": ["content_scripts/parseScore.js", "content_scripts/content.js"]
        }
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "host_permissions": [
        "https://gitee.com/*"
    ],
    "permissions": ["sidePanel","tabs", "scripting", "clipboardWrite"]
}
